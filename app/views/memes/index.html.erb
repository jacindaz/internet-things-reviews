<ul class="small-block-grid-2 medium-block-grid-3 large-block-grid-4">
  <% @memes.each do |meme| %>
    <li>
      <%= link_to meme.name, meme_path(meme.id) %> (<%= meme.average_rating %>)<br>
      <%= link_to (image_tag meme.picture.thumbnail), meme_path(meme.id) %>
    </li>
  <% end %>
</ul>

<%= paginate @memes %>
